@model DataMenuViewModel

    <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" role="button"
           data-bs-toggle="dropdown"
           aria-expanded="false">
            Gezondheid logs
        </a>
        <ul class="dropdown-menu">
            @{
                foreach (var logbook in Model.ChoosenLogbooks)
                {
                    <li><a asp-action="Diary" asp-controller="Logs" asp-route-logbookId="@logbook.Id"
                            class="dropdown-item">@logbook.Name</a></li>
                }
            }

            <li> <hr class="dropdown-divider"> </li>
            <li><a class="dropdown-item" asp-action="Options" asp-controller="User">Instellingen</a></li>
        </ul>
    </li>





